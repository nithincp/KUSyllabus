                    android:textStyle="bold" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="- recursive functions"
                    android:id="@+id/textView1874"
                    android:layout_gravity="center_horizontal"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="- passing arrays and strings to functions"
                    android:id="@+id/textView1873"
                    android:layout_gravity="center_horizontal"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="4. Structures and unions"
                    android:id="@+id/textView132"
                    android:layout_gravity="left|center_horizontal"
                    android:textStyle="bold" />

                <TextView
