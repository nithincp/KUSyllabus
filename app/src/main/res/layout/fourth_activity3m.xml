    <TextView
        android:layout_width="376dp"
        android:layout_height="wrap_content"
        android:text="Mechanics of Solids"
        android:id="@+id/textView816"
        android:layout_weight="0.06"
        android:textSize="@dimen/abc_dialog_padding_material"
        style="?android:attr/buttonStyle"

        android:textColor="@color/dim_foreground_material_light"
        android:textStyle="bold"
        android:gravity="center_horizontal"
        android:layout_gravity="center" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Computer Programming and Numerical Methods"
        android:id="@+id/textView817"
        android:layout_weight="0.06"
        android:gravity="center_horizontal"
        style="?android:attr/buttonStyle"

        android:textColor="@color/dim_foreground_material_light"
        android:textSize="@dimen/abc_dialog_padding_material"
